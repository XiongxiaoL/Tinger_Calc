                android:layout_height="wrap_content"
                />
            <TextView
                android:id="@+id/tv_11"
                android:drawableLeft="@drawable/ney2"
                android:textColor="@color/black"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:gravity="center_vertical|left"
                android:layout_height="wrap_content"
                />

        </TableRow>
        <TableRow
            android:layout_marginLeft="10dp"
            android:layout_marginBottom="10dp"
            >
            <TextView
                android:id="@+id/tv_12"
                android:gravity="center_vertical|left"
                android:drawableLeft="@drawable/lxx"
                android:textColor="@color/black"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                />
            <TextView
                android:id="@+id/tv_13"
                android:drawableLeft="@drawable/lxy"
                android:layout_width="0dp"
                android:textColor="@color/black"
                android:layout_weight="1"
                android:gravity="center_vertical|left"
                android:layout_height="wrap_content"
                />
            <TextView
                android:id="@+id/tv_14"
                android:drawableLeft="@drawable/lyy"
                android:layout_width="0dp"
                android:textColor="@color/black"
                android:layout_weight="1"
                android:gravity="center_vertical|left"
                android:layout_height="wrap_content"
                />

        </TableRow>
        <TableRow
            android:layout_marginLeft="10dp"
            android:layout_marginBottom="10dp"
            >
            <TextView
                android:id="@+id/tv_15"
                android:drawableLeft="@drawable/beta1"
                android:textColor="@color/black"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:gravity="center_vertical|left"
                android:layout_height="wrap_content"
                />
            <TextView
                android:id="@+id/tv_16"
                android:drawableLeft="@drawable/beta0"
                android:layout_width="0dp"
                android:textColor="@color/black"
                android:gravity="center_vertical|left"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                />


        </TableRow>
        <TextView
            android:id="@+id/tv_17"
            android:gravity="center"
            android:layout_width="0dp"
            android:textColor="@color/black"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            />
    </TableLayout>
</RelativeLayout>