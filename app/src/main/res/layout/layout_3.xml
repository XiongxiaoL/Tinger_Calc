<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <!--    第一组-->
    <RelativeLayout
        android:id="@+id/data_1"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/clear1"
            android:text="@string/clear"
            android:onClick="clear"
            android:layout_centerVertical="true"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="5dp"
            android:textColor="@color/black"
            android:layout_alignParentLeft="true"
            android:layout_marginRight="5dp"
            android:background="@drawable/shape_border"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <EditText
            android:id="@+id/et_1"
            android:hint="@string/dataX"
            android:gravity="left"
            android:layout_centerHorizontal="true"
            android:minLines="3"
            android:layout_marginRight="5dp"
            android:maxLines="3"
            android:background="@drawable/shape_border"
            android:textColor="@color/black"

            android:layout_toRightOf="@id/clear1"
           android:layout_width="match_parent"
            android:layout_height="100dp"/>
         </RelativeLayout>

    <!--    第2组-->
    <RelativeLayout
        android:id="@+id/data_2"
        android:layout_marginTop="10dp"
        android:layout_below="@id/data_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/clear2"
            android:text="@string/clear"
            android:onClick="clear"
            android:layout_centerVertical="true"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="5dp"
            android:textColor="@color/black"
            android:layout_alignParentLeft="true"
            android:layout_marginRight="5dp"
            android:background="@drawable/shape_border"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <EditText
            android:id="@+id/et_2"
            android:hint="@string/dataY"
            android:layout_marginRight="5dp"
            android:gravity="left"
            android:layout_centerHorizontal="true"
            android:minLines="3"
            android:maxLines="3"
            android:background="@drawable/shape_border"
            android:textColor="@color/black"
            android:layout_toRightOf="@id/clear2"
            android:layout_width="match_parent"
            android:layout_height="100dp"/>
        </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rl12"
        android:layout_below="@id/data_2"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <Button

            android:text="@string/ok"
            android:id="@+id/submit"
            android:layout_marginRight="5dp"
            android:background="@drawable/shape_border"
            android:checked="true"
            android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/tip"
            android:text="@string/computerTable"
            android:textColor="@color/black"
            android:layout_marginTop="5dp"
            android:layout_centerHorizontal="true"
            android:layout_below="@id/submit"
            android:layout_marginBottom="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
<TextView
    android:layout_below="@id/tip"
    android:layout_marginBottom="5dp"
    android:background="#434242"
    android:layout_width="match_parent"
    android:layout_height="1dp"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_below="@id/rl12"
        android:id="@+id/rl3"
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        android:layout_height="wrap_content">

    </RelativeLayout>
    <TableLayout
        android:layout_below="@id/rl3"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TableRow
            android:layout_marginLeft="10dp"
            android:layout_marginBottom="10dp"
            >
            <TextView
                android:id="@+id/tv_1"
                android:gravity="center_vertical|left"
                android:drawableLeft="@drawable/sum_x"
                android:textColor="@color/black"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                />
            <TextView
                android:id="@+id/tv_2"
                android:text="n="
                android:gravity="center_vertical|left"
                android:layout_width="0dp"
                android:textSize="20dp"
                android:textColor="@color/black"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                />
            <TextView
                android:id="@+id/tv_3"
                android:drawableLeft="@drawable/sum_y"
                android:layout_width="0dp"
                android:gravity="center_vertical|left"
                android:textColor="@color/black"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                />

        </TableRow>
        <TableRow
            android:layout_marginLeft="10dp"
            android:layout_marginBottom="10dp"
            >
            <TextView
                android:id="@+id/tv_4"
                android:drawableLeft="@drawable/ex"
                android:textColor="@color/black"
                android:layout_width="0dp"
                android:gravity="center_vertical|left"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                />
            <TextView
                android:id="@+id/tv_kk"
                android:visibility="invisible"
                android:drawableLeft="@drawable/ex"
                android:textColor="@color/black"
                android:layout_width="0dp"
                android:gravity="center_vertical|left"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                />
            <TextView
                android:id="@+id/tv_5"
                android:drawableLeft="@drawable/ey"
                android:layout_width="0dp"
                android:gravity="center_vertical|left"
                android:textColor="@color/black"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                />

        </TableRow>
        <TableRow
            android:layout_marginLeft="10dp"
            android:layout_marginBottom="10dp"
            >
            <TextView
                android:gravity="center_vertical|left"
                android:id="@+id/tv_6"
                android:drawableLeft="@drawable/sum_x2"
                android:textColor="@color/black"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                />
            <TextView
                android:id="@+id/tv_7"
                android:drawableLeft="@drawable/sum_xiyi"
                android:layout_width="0dp"
                android:gravity="center_vertical|left"
                android:textColor="@color/black"
